# AI Service Configuration
AI_DATA_PATH=/path/to/ai/data
MODEL_CACHE_PATH=/path/to/model/cache

# Authentication & Security
CLOUDFLARE_TOKEN=
API_SECRET_KEY=

# Redis Configuration
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_MAXMEMORY=4gb
REDIS_MAXMEMORY_POLICY=allkeys-lru
REDIS_TLS_ENABLED=true

# Grafana Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=
GRAFANA_PORT=3000

# Domain Settings
DOMAIN=statista.live
API_SUBDOMAIN=api.statista.live
MONITOR_SUBDOMAIN=monitor.statista.live

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=ai_server

# Prometheus Configuration
PROMETHEUS_PORT=9090
PROMETHEUS_BASIC_AUTH_USER=
PROMETHEUS_BASIC_AUTH_PASS=

# GPU Settings
NVIDIA_VISIBLE_DEVICES=0
NVIDIA_DRIVER_CAPABILITIES=compute,utility
CUDA_VERSION=12.3.1

# API Configuration
API_PORT=8000
MAX_REQUEST_SIZE=100MB
RATE_LIMIT_PER_MINUTE=60